<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WhatBenchPress">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="415dp"
        android:layout_height="560dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/WhatAboutBenchPress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:layout_width="388dp"
                android:layout_height="wrap_content"
                android:text="@string/WhatAboutBenchPrees"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="match_parent"
                android:layout_height="300dp"
                app:srcCompat="@drawable/whataboutbenchpress" />

            <TextView
                android:layout_width="388dp"
                android:layout_height="wrap_content"
                android:text="@string/WhatAboutBenchPrees2"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/notgoodbenchprees" />


        </LinearLayout>


    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>